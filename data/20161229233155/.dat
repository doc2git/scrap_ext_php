/* Code tidied up by ScrapBook */
@charset "utf-8";
html { color: rgb(51, 51, 51); background: rgb(255, 255, 255) none repeat scroll 0% 0%; }
article, aside, blockquote, body, button, code, dd, details, div, dl, dt, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, hr, input, legend, li, menu, nav, ol, p, pre, section, td, textarea, th, ul { margin: 0px; padding: 0px; }
article, aside, details, figcaption, figure, footer, header, hgroup, iframe, menu, nav, section { display: block; }
body, button, input, select, textarea { font: 12px/1.5 "微软雅黑","Microsoft Yahei","Hiragino Sans GB",tahoma,arial,"宋体"; }
input, select, textarea { font-size: 100%; }
fieldset, img { border: 0px none; }
i { font-style: normal; }
address, caption, cite, code, dfn, em, th, var { font-style: normal; font-weight: 500; }
ol, ul { list-style: outside none none; }
h1, h2, h3, h4, h5, h6 { font-weight: 500; color: rgb(0, 0, 0); }
h1 { font-size: 24px; }
h2 { font-size: 16px; }
h3, h4, h5, h6 { font-size: 14px; }
a, ins { text-decoration: none; }
.y-lnk, .y-lnk-gray, .y-lnk-highlight, a { transition: color 0.2s ease 0s; }
a, a:hover, a:link, a:visited { color: rgb(0, 162, 202); }
.y-lnk-gray:hover, .y-lnk-highlight:hover, .y-lnk:hover, a:hover { text-decoration: underline; }
.y-clear::after { visibility: hidden; display: block; font-size: 0px; content: " "; clear: both; height: 0px; }
.y-clear {  }
.y-align-center { text-align: center ! important; }
.y-left, .y-right { display: inline; }
.y-left { float: left ! important; }
.y-right { float: right ! important; }
.y-row { min-width: 1000px; max-width: 1200px; margin-left: auto; margin-right: auto; }
.y-row::after { visibility: hidden; display: block; font-size: 0px; content: " "; clear: both; height: 0px; }
.y-row .y-span1, .y-row .y-span10, .y-row .y-span11, .y-row .y-span12, .y-row .y-span2, .y-row .y-span3, .y-row .y-span4, .y-row .y-span5, .y-row .y-span6, .y-row .y-span7, .y-row .y-span8, .y-row .y-span9 { float: left; display: inline; margin-right: 2%; }
.y-row .y-span3 { width: 23.5%; }
.y-row .y-span9 { width: 74.5%; }
.y-row .y-last { margin-right: 0px ! important; }
.y-btn-anti, .y-btn-blue, .y-btn-trans, .y-btn-white, a.y-btn-anti, a.y-btn-blue, a.y-btn-trans, a.y-btn-white { transition: all 0.3s ease 0s; }
.y-btn-blue { color: rgb(255, 255, 255); }
.y-btn-anti, .y-btn-blue, .y-btn-trans, .y-btn-white { display: inline-block; min-width: 140px; height: 36px; padding: 0px 30px 1px; background: rgb(0, 162, 202) none repeat scroll 0% 0%; border: 1px solid rgb(0, 162, 202); line-height: 32px; font-size: 14px; text-align: center; outline: medium none; cursor: pointer; border-radius: 0px; }
.y-btn-blue:hover, .y-btn-white:hover { background: rgb(51, 181, 212) none repeat scroll 0% 0%; border-color: rgb(51, 181, 212); }
.y-btn-blue:active, .y-btn-white:active { background: rgb(0, 143, 178) none repeat scroll 0% 0%; border-color: rgb(0, 143, 178); }
.y-btn-anti, .y-btn-white { background: rgb(255, 255, 255) none repeat scroll 0% 0%; border-color: rgb(0, 162, 202); color: rgb(0, 162, 202); }
.y-btn-white:active, .y-btn-white:hover { color: rgb(255, 255, 255); }
a.y-btn-blue, a.y-btn-blue:link, a.y-btn-blue:visited { color: rgb(255, 255, 255); }
a.y-btn-anti, a.y-btn-anti:link, a.y-btn-anti:visited, a.y-btn-blue, a.y-btn-blue:link, a.y-btn-blue:visited, a.y-btn-trans, a.y-btn-trans:link, a.y-btn-trans:visited, a.y-btn-white, a.y-btn-white:link, a.y-btn-white:visited { display: inline-block; min-width: 78px; height: 33px; padding: 0px 30px 1px; background: rgb(0, 162, 202) none repeat scroll 0% 0%; line-height: 33px; font-size: 14px; text-align: center; outline: medium none; border-radius: 0px; }
a.y-btn-blue:hover { background: rgb(51, 181, 212) none repeat scroll 0% 0%; border-color: rgb(51, 181, 212); text-decoration: none; }
a.y-btn-blue:active { background: rgb(0, 143, 178) none repeat scroll 0% 0%; border-color: rgb(0, 143, 178); }
a.y-btn-white, a.y-btn-white:link, a.y-btn-white:visited { background: rgb(255, 255, 255) none repeat scroll 0% 0%; border-color: rgb(0, 162, 202); color: rgb(0, 162, 202); }
a.y-btn-white:hover { background: rgb(51, 181, 212) none repeat scroll 0% 0%; border-color: rgb(51, 181, 212); color: rgb(255, 255, 255); text-decoration: none; }
a.y-btn-white:active { background: rgb(0, 143, 178) none repeat scroll 0% 0%; border-color: rgb(0, 143, 178); color: rgb(255, 255, 255); }
@font-face {
  font-family: "iconfont";
  font-style: normal;
  font-weight: 400;
  src: url("iconfont.eot") format("embedded-opentype"), url("iconfont.woff") format("woff"), url("iconfont.ttf") format("truetype"), url("iconfont.svg") format("svg");
}
[class*=" icon-"], [class^="icon-"] { font-family: "iconfont"; font-style: normal; font-weight: 400; font-variant: normal; text-transform: none; line-height: 1; }
.icon-down3::before { content: ""; }
.icon-menu::before { content: ""; }
.icon-Search::before { content: ""; }
.icon-up3::before { content: ""; }
.copyright-100 { clear: both; background: rgb(248, 248, 248) none repeat scroll 0% 0%; border-top: 1px solid rgb(229, 229, 229); min-width: 1000px; }
.copyright-100 .copyright { text-align: left; color: rgb(153, 153, 153); font-size: 12px; padding: 15px 0px 40px; }
.copyright-100 .copyright p { padding: 5px 0px; }
.copyright-100 .copyright p a { padding: 0px 5px 0px 0px; color: rgb(179, 179, 179); white-space: nowrap; }
.copyright-100 .copyright p .link-item { padding: 0px 10px 0px 0px; }
.copyright-100 .copyright .link-wrap { line-height: 24px; }
.copyright-100 .copyright p.big { font-size: 14px; }
.copyright-100 .copyright p.big a { color: rgb(102, 102, 102); }
.copyright-100 .copyright .copyright { margin-top: -6px; }
.float-tool { position: fixed; bottom: 33%; right: 0.1%; z-index: 250; margin-bottom: -90px; }
